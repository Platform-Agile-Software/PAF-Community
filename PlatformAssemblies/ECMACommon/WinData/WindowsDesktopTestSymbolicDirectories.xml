<?xml version="1.0"?>
<doc>
    <platform>
        <name>WindowsDesktop</name>
    </platform>
    <symbolicdirectorymappings>
		<!-- check on a physical mapping -->
        <mapping symbol="SpecialFiles" mapto="c:/specialfiles"/>
		<!-- check on a virtual mapping, assuming "Documents" is created ahead of time. -->
        <mapping symbol="SpecialFilesInDocuments" mapto="Documents:/specialfiles"/>
    </symbolicdirectorymappings>
</doc>
